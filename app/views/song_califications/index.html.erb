<p id="notice"><%= notice %></p>

<h1>Song Califications</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Song</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @song_califications.each do |song_calification| %>
      <tr>
        <td><%= song_calification.user_id %></td>
        <td><%= song_calification.song_id %></td>
        <td><%= song_calification.grade %></td>
        <td><%= link_to 'Show', song_calification %></td>
        <td><%= link_to 'Edit', edit_song_calification_path(song_calification) %></td>
        <td><%= link_to 'Destroy', song_calification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song Calification', new_song_calification_path %>
