<section id="one">
    <div class="inner">
        <p id="notice"><%= notice %></p>

        <h1>Users</h1>
        <table>
          <thead>
            <tr>
              <th></th>
              <th>Username</th>
              <th>Mail</th>
              
            </tr>
          </thead>

          <tbody>
            <% @users.each do |user| %>
              <tr>
                <td><%= image_tag(user.pic.url, size: 100) %></td>
                <td><%= link_to user.username, user_profile_path(:id => user.id) %></td>
                <td><%= user.email %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
    </div>
</section>
