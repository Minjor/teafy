<p id="notice"><%= notice %></p>

<h1>Artist Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>User</th>
      <th>Review</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artist_reviews.each do |artist_review| %>
      <tr>
        <td><%= artist_review.artist_id %></td>
        <td><%= artist_review.user_id %></td>
        <td><%= artist_review.review %></td>
        <td><%= link_to 'Show', artist_review %></td>
        <td><%= link_to 'Edit', edit_artist_review_path(artist_review) %></td>
        <td><%= link_to 'Destroy', artist_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist Review', new_artist_review_path %>
